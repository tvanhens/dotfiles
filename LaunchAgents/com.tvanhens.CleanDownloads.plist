  <key>StandardOutPath</key>
  <string>/Users/tvanhens/var/log/tvanhens_clean_downloads.log</string>
  <key>StandardErrorPath</key>
  <string>/Users/tvanhens/var/log/tvanhens_clean_downloads_errors.log</string>
</dict>
</plist>
